	public static void main(String[] args) {
		SpringApplication.run(CrudMysqlApplication.class, args);
	
	    Book b1 = new Book();
	    b1.setAuthor("sanjay baru");
	    b1.setTitle("Accidental PrimeMinister");
	    b1.setId(1);
	    b1.setPrice(1300);
	    
	    bookService.
	
	}

}
